<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>

<h1><%= title %></h1>
<br><br>
<%if (data.length > 0) { %>
  <table border = "1">
    <col width="240">
    <col width="200">
    <col width="220">
    <col width="200">
    <col width="350">

      <tr>
        <th>Movie</th>
        <th>Theater</th>
        <th>Company</th>
        <th>Card#</th>
        <th>View Date</th>

      </tr>
    <% for (var i = 0; i < data.length; i++) { %>
    <tr>
      <td><%= data[i].movName %></td>
      <td><%= data[i].thName %></td>
      <td><%= data[i].comName%></td>
      <td><%= data[i].CreditCardNum %></td>
      <td><%= data[i].movPlayDate %></td>
    </tr>
    <% } %>
  </table>
<% } else {%>
  <table border = "1">
    <col width="240">
    <col width="200">
    <col width="220">
    <col width="200">
    <col width="350">
    <tr>
      <th>Movie</th>
      <th>Theater</th>
      <th>Company</th>
      <th>Card#</th>
      <th>View Date</th>
    </tr>
    <% for (var i = 0; i < 5; i++) { %>
      <tr>
        <td>&nbsp;</td>
        <td>&nbsp;</td>
        <td>&nbsp;</td>
        <td>&nbsp;</td>
        <td>&nbsp;</td>
      </tr>
    <% } %>
  </table>
<% } %>
<br><br>

<form action="/" method="get">
<button type="submit" class="btn btn-primary" value="Back">Back</button>
</form>

</body>
</html>
