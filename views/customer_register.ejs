<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>

<h1><%= title %></h1>

<form name="customer_register" method="POST">
  First Name:
  <input type="text" name="fname">
  Last Name:
  <input type="text" name="lname"><br>
  <br>
  Username:
  <input type="text" name="username"><br>
  <br>
  Password:
  <input type="text" name="password">
  Confirm Password:
  <input type="text" name="cpassword"><br>
  
  <br>
  Credit Card#
  <br>
  <% data.forEach(function(d) { %>
      <%= d %> <br/>
  <% }); %>
  <!--<p> <%= data %> </p>-->
  <br>
  <textarea name="cardNumber" cols="25" rows="1"></textarea>
  <button type="submit" class="btn btn-primary" value="add" name="select">Add</button>

  <button type="submit" class="btn btn-primary" value="register" name="select">Register</button>
  <br>

  
</form>

<form action="/customerRegister/addCreditCard" name="creditcard" method="POST" >
    


</form>

<form action="/nevigate" method="get">
    <button type="submit" class="btn btn-primary btn-lg">Back</button>
  </form>

<%if (errors.length > 0) { %>
  <ul>
    <% for (var i = 0; i < errors.length; i++) { %>
      <li>
        <%=errors[i].msg%>
      </li>
    <% } %>
  </ul>
<% } %>

</body>
</html>